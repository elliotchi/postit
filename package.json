{
  "name": "remmutable",
  "version": "1.0.0",
  "description": "react js course",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 4001",
    "lint": "eslint src/.; exit 0",
    "fix": "eslint --fix src/. exit 0",
    "start:server": "nodemon server/src/server.js --exec babel-node --presets es2015",
    "build:server": "babel --presets es2015 server/src --out-dir server/dist",
    "production": "webpack -p && npm run build:server && node server/dist/server"

  },
  "author": "elliot chi",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^6.0.4",
    "express": "^4.13.4",
    "path": "^0.12.7",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "html-webpack-plugin": "^2.16.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
